{
  "author": "Snowcoders",
  "description": "Documentation site for Sortier",
  "devDependencies": {
    "@snowcoders/renovate-config": "2.0.1",
    "@snowcoders/sortier": "3.2.0",
    "@types/classnames": "2.2.11",
    "@types/highlight.js": "9.12.4",
    "@types/react": "17.0.0",
    "@types/react-dom": "17.0.0",
    "@types/react-router-dom": "5.1.6",
    "@types/react-router-hash-link": "1.2.1",
    "@types/webpack": "4.41.25",
    "acorn": "8.0.4",
    "changelog-updater": "2.0.2",
    "classnames": "2.2.6",
    "concurrently": "5.3.0",
    "copyfiles": "2.4.1",
    "cross-env": "7.0.3",
    "css-loader": "5.0.1",
    "highlight.js": "10.4.1",
    "husky": "4.3.4",
    "lint-staged": "10.2.11",
    "live-server": "1.2.1",
    "node-sass": "4.14.1",
    "prettier": "2.1.0",
    "react": "17.0.1",
    "react-dom": "17.0.1",
    "react-router-dom": "5.2.0",
    "react-router-hash-link": "2.2.2",
    "rimraf": "3.0.2",
    "sass-loader": "10.0.0",
    "source-map-loader": "1.1.3",
    "style-loader": "2.0.0",
    "ts-loader": "8.0.11",
    "ts-node": "9.1.0",
    "tsconfig-paths": "3.9.0",
    "tslib": "2.0.3",
    "typescript": "4.1.2",
    "webpack": "5.10.0",
    "webpack-cli": "4.2.0"
  },
  "license": "MIT",
  "main": "dist/main.js",
  "name": "@snowcoders/sortier-documentation",
  "repository": "https://github.com/snowcoders/sortier-documentation",
  "scripts": {
    "build": "cross-env TS_NODE_PROJECT=\"tsconfig.webpack.json\" webpack --config=\"webpack.config.prod.ts\"",
    "build:dev": "cross-env TS_NODE_PROJECT=\"tsconfig.webpack.json\" webpack --config=\"webpack.config.dev.ts\"",
    "clean": "rimraf dist",
    "prepare": "npm run clean && npm run build",
    "serve": "live-server --port=53002 --host=localhost --watch=dist,index.html",
    "start": "concurrently --kill-others \"npm run watch\" \"npm run serve\"",
    "version": "changelog-updater && git add .",
    "watch": "cross-env TS_NODE_PROJECT=\"tsconfig.webpack.json\" webpack --watch --config=\"webpack.config.dev.ts\""
  },
  "version": "1.0.4"
}
