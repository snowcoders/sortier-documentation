  "author": "Snowcoders",
  "description": "Documentation site for Sortier",
  "devDependencies": {
    "@snowcoders/renovate-config": "1.0.4",
    "@snowcoders/sortier": "2.6.2",
    "@types/classnames": "2.2.9",
    "@types/highlight.js": "9.12.3",
    "@types/react": "16.9.2",
    "@types/react-dom": "16.9.0",
    "@types/react-router-dom": "4.3.5",
    "@types/react-router-hash-link": "1.2.1",
    "@types/webpack": "4.39.1",
    "acorn": "7.0.0",
    "classnames": "2.2.6",
    "concurrently": "4.1.2",
    "copyfiles": "2.1.1",
    "cross-env": "5.2.0",
    "css-loader": "3.2.0",
    "highlight.js": "9.15.10",
    "husky": "3.0.4",
    "lint-staged": "9.2.3",
    "live-server": "1.2.1",
    "node-sass": "4.12.0",
    "prettier": "1.18.2",
    "react": "16.9.0",
    "react-dom": "16.9.0",
    "react-router-dom": "5.0.1",
    "react-router-hash-link": "1.2.2",
    "rimraf": "3.0.0",
    "sass-loader": "7.3.1",
    "source-map-loader": "0.2.4",
    "style-loader": "1.0.0",
    "ts-loader": "6.0.4",
    "ts-node": "8.3.0",
    "tsconfig-paths": "3.8.0",
    "tslib": "1.10.0",
    "typescript": "3.5.3",
    "webpack": "4.39.2",
    "webpack-cli": "3.3.7"
  },
  "license": "MIT",
  "main": "dist/main.js",
  "name": "@snowcoders/sortier-documentation",
  "repository": "https://github.com/snowcoders/sortier-documentation",
  "scripts": {
    "build": "cross-env TS_NODE_PROJECT=\"tsconfig.webpack.json\" webpack --config=\"webpack.config.prod.ts\"",
    "build:dev": "cross-env TS_NODE_PROJECT=\"tsconfig.webpack.json\" webpack --config=\"webpack.config.dev.ts\"",
    "clean": "rimraf dist",
    "prepare": "npm run clean && npm run build",
    "serve": "live-server --port=53002 --host=localhost --watch=dist,index.html",
    "start": "concurrently --kill-others \"npm run watch\" \"npm run serve\"",
    "watch": "cross-env TS_NODE_PROJECT=\"tsconfig.webpack.json\" webpack --watch --config=\"webpack.config.dev.ts\""
  },
  "version": "1.0.3"
}
